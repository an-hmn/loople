<h1>All Loops</h1>
<ul>
  <% @loops.each do |loop| %>
    <li><strong><%= link_to(loop.name, loop) %></strong></li>
    <li>ID <%= loop.id %> - Colour <%= loop.colour %></li>
    <li>
      <ul>
        <% loop.keystrokes.each do |stroke| %>
          <li>Key Pressed: <%= stroke.key %></li>
          <li>At Time: <%= stroke.time %></li>
        <% end %>
      </ul>
    </li>
  <% end %>
</ul>